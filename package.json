{
  "name": "@interest-protocol/clamm-sdk",
  "version": "1.0.0",
  "author": "jose cerqueira",
  "files": [
    "dist"
  ],
  "description": "The SDK for Interest Protocol Sui CLAMM",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "new-stable-2-pool": "npx ts-node ./src/scripts/stable-pool/new-stable-2-pool.script.ts",
    "clean": "rimraf dist && rimraf tsconfig.tsbuildinfo",
    "build:tsup": "tsup ./src/index.ts --format esm,cjs --sourcemap",
    "build:types": "tsc --project tsconfig.json && resolve-tspaths",
    "build": "npx run build:types && npx run build:tsup",
    "prepublishOnly": "npx run clean && npx run build"
  },
  "keywords": [
    "sui",
    "move",
    "sdk",
    "interest-protocol",
    "ipx"
  ],
  "license": "MIT",
  "dependencies": {
    "@mysten/sui.js": "^0.51.2",
    "ramda": "^0.29.1",
    "tiny-invariant": "^1.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/ramda": "^0.29.12",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "dotenv": "^16.4.5",
    "eslint": "^9.1.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "prettier": "^3.2.5",
    "resolve-tspaths": "^0.8.18",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  }
}
